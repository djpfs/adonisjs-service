{
  "name": "@djpfs/adonisjs-service",
  "version": "1.0.2",
  "description": "Command to create and register service for AdonisJs v5",
  "files": [
    "build/commands",
    "build/templates",
    "build/instructions.js",
    "build/instructions.d.ts"
  ],
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "pretest": "npm run lint",
    "test": "node japaFile.js",
    "clean": "del build",
    "compile": "npm run lint:fix && npm run clean && tsc",
    "copyfiles": "copyfiles \"templates/**/*.txt\" build",
    "build": "npm run compile && npm run copyfiles",
    "commit": "git-cz",
    "release": "np",
    "version": "npm run build",
    "format": "prettier --write .",
    "prepublishOnly": "npm run build",
    "lint": "eslint . --ext=.ts",
    "lint:fix": "eslint . --ext=.ts --fix",
    "sync-labels": "github-label-sync --labels ./node_modules/@adonisjs/mrm-preset/gh-labels.json djpfs/adonisjs-service"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djpfs/adonisjs-service.git"
  },
  "keywords": [
    "adonisjs",
    "adonis",
    "adonis5",
    "adonis5-service",
    "adonis5-service-generator",
    "adonis-service"
  ],
  "author": "djpfs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/djpfs/adonisjs-service/issues"
  },
  "homepage": "https://github.com/djpfs/adonisjs-service#readme",
  "devDependencies": {
    "@adonisjs/application": "^5.1.3",
    "@adonisjs/mrm-preset": "^4.1.0",
    "@adonisjs/require-ts": "^2.0.4",
    "@poppinss/dev-utils": "^1.1.4",
    "@types/node": "^15.6.1",
    "del-cli": "^3.0.1",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-adonis": "^1.3.1",
    "eslint-plugin-prettier": "^3.4.0",
    "github-label-sync": "^2.0.0",
    "husky": "^6.0.0",
    "japa": "^3.1.1",
    "mrm": "^3.0.2",
    "np": "^7.5.0",
    "prettier": "^2.3.0",
    "typescript": "^4.3.2"
  },
  "nyc": {
    "exclude": [
      "test"
    ],
    "extension": [
      ".ts"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "node ./node_modules/@adonisjs/mrm-preset/validateCommit/conventional/validate.js"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@adonisjs/core": "^5.9.0",
    "@adonisjs/sink": "^5.1.5",
    "copyfiles": "^2.4.1"
  },
  "peerDependencies": {
    "@adonisjs/application": "^5.0.0"
  },
  "np": {
    "contents": ".",
    "anyBranch": false
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "adonisjs": {
    "instructions": "./build/instructions.js",
    "instructionsMd": "./build/instructions.md"
  }
}
